<% if @warehouse.errors.any? %>
    <ul>
        <% @warehouse.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
    </ul>
<% end %>

<h1>Editar Galp√£o <%= @warehouse.name %></h1>

<%= form_with(model: @warehouse) do |f| %>
    <div>
        <%= f.label :name%>
        <%= f.text_field :name %>
    </div>

    <div>
        <%= f.label :city%>
        <%= f.text_field :city %>
    </div>

    <div>
        <%= f.label :code%>
        <%= f.text_field :code %>
    </div>

    <div>
        <%= f.label :adress%>
        <%= f.text_field :adress %>
    </div>

    <div>
        <%= f.label :cep%>
        <%= f.text_field :cep %>
    </div>

    <div>
        <%= f.label :area%>
        <%= f.text_field :area %>
    </div>

    <div>
        <%= f.label :description %>
        <%= f.text_area :description %>
    </div>

    <div class="button">
        <%= f.submit 'Enviar' %>
    </div>

<% end %>